{
  "name": "organizee",
  "version": "1.0.0",
  "description": "organizee is a command-line interface (CLI) tool designed to help users organize their files efficiently based on file type.",
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "splitting": false,
    "sourcemap": true,
    "clean": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ab-elhaddad/organizee"
  },
  "scripts": {
    "build": "tsup",
    "start": "node dist/index.mjs",
    "watch": "jest --watchAll --verbose",
    "test": "jest",
    "prepare": "husky",
    "spell-check": "cspell \"**.ts\""
  },
  "bin": "dist/index.mjs",
  "author": "Abdelrrahman Elhaddad",
  "license": "MIT",
  "keywords": [
    "organize",
    "files",
    "organizee"
  ],
  "dependencies": {
    "chalk": "^5.3.0",
    "figlet": "^1.7.0"
  },
  "devDependencies": {
    "@types/figlet": "^1.5.8",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.7",
    "cspell": "^8.7.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  }
}
